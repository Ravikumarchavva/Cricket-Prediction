{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "deliveries",
						"type": "DatasetReference"
					},
					"name": "deliveries"
				},
				{
					"dataset": {
						"referenceName": "matches",
						"type": "DatasetReference"
					},
					"name": "matches"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "joinedData",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "join1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          match_id as string,",
				"          season as string,",
				"          start_date as string,",
				"          venue as string,",
				"          innings as string,",
				"          ball as string,",
				"          batting_team as string,",
				"          bowling_team as string,",
				"          striker as string,",
				"          non_striker as string,",
				"          bowler as string,",
				"          runs_off_bat as string,",
				"          extras as string,",
				"          wides as string,",
				"          noballs as string,",
				"          byes as string,",
				"          legbyes as string,",
				"          penalty as string,",
				"          wicket_type as string,",
				"          player_dismissed as string,",
				"          other_wicket_type as string,",
				"          other_player_dismissed as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> deliveries",
				"source(output(",
				"          team1 as string,",
				"          team2 as string,",
				"          gender as string,",
				"          season as string,",
				"          date as string,",
				"          venue as string,",
				"          city as string,",
				"          toss_winner as string,",
				"          toss_decision as string,",
				"          winner as string,",
				"          match_id as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> matches",
				"deliveries, matches join(deliveries@match_id == matches@match_id,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join1",
				"join1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink1"
			]
		}
	}
}